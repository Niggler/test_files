 <Worksheet ss:Name="Sheet1">
  <Table ss:ExpandedColumnCount="5" ss:ExpandedRowCount="18" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="12">
   <Column ss:AutoFitWidth="0" ss:Width="235"/>
   <Column ss:AutoFitWidth="0" ss:Width="130"/>
   <Column ss:AutoFitWidth="0" ss:Width="154"/>
   <Column ss:AutoFitWidth="0" ss:Width="91"/>
   <Column ss:AutoFitWidth="0" ss:Width="131"/>
   <Row>
    <Cell ss:StyleID="s34"><Data ss:Type="String">&lt;jx:forEach items=&quot;${departments}&quot; var=&quot;department&quot;&gt;</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="18" ss:StyleID="s23">
    <Cell ss:StyleID="s21"><Data ss:Type="String">Department</Data></Cell>
    <Cell ss:StyleID="s21"><Data ss:Type="String">${department.name}</Data></Cell>
    <Cell ss:StyleID="s22"/>
    <Cell ss:StyleID="s22"/>
    <Cell ss:StyleID="s22"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s24"><Data ss:Type="String">Chief</Data></Cell>
   </Row>
   <Row ss:StyleID="s26">
    <Cell ss:StyleID="s25"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Age</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s25"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="Default"/>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">${department.chief.name}</Data></Cell>
    <Cell><Data ss:Type="String">${department.chief.age}</Data></Cell>
    <Cell><Data ss:Type="String">${department.chief.payment}</Data></Cell>
    <Cell ss:StyleID="s27"><Data ss:Type="String">${department.chief.bonus}</Data></Cell>
   </Row>
   <Row ss:StyleID="s26">
    <Cell ss:StyleID="s28"/>
    <Cell ss:StyleID="s28"/>
    <Cell ss:StyleID="s28"/>
    <Cell ss:Index="5" ss:StyleID="s28"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s29"><Data ss:Type="String">Employees</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s30"><Data ss:Type="String">Name</Data></Cell>
    <Cell ss:StyleID="s30"><Data ss:Type="String">Age</Data></Cell>
    <Cell ss:StyleID="s30"><Data ss:Type="String">Payment</Data></Cell>
    <Cell ss:StyleID="s30"><Data ss:Type="String">Bonus</Data></Cell>
    <Cell ss:StyleID="s30"><Data ss:Type="String">Total</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s36"><Data ss:Type="String">&lt;jx:forEach items=&quot;${department.staff}&quot; var=&quot;employee&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s36"><Data ss:Type="String">&lt;jx:if test=&quot;${employee.payment &lt; 2000}&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s38"><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell ss:StyleID="s38"><Data ss:Type="String">${employee.age}</Data></Cell>
    <Cell ss:StyleID="s38"><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s40"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell ss:StyleID="s38"><Data ss:Type="String">$[C#*(1+D#)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s36"><Data ss:Type="String">&lt;/jx:if&gt;</Data></Cell>
    <Cell ss:Index="4" ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s36"><Data ss:Type="String">&lt;jx:if test=&quot;${employee.payment &gt;= 2000}&quot;&gt;</Data></Cell>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
    <Cell ss:StyleID="s35"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s39"><Data ss:Type="String">${employee.name}</Data></Cell>
    <Cell ss:StyleID="s39"><Data ss:Type="String">${employee.age}</Data></Cell>
    <Cell ss:StyleID="s39"><Data ss:Type="String">${employee.payment}</Data></Cell>
    <Cell ss:StyleID="s41"><Data ss:Type="String">${employee.bonus}</Data></Cell>
    <Cell ss:StyleID="s39"><Data ss:Type="String">$[C#*(1+D#)]</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="s36"><Data ss:Type="String">&lt;/jx:if&gt;</Data></Cell>
    <Cell ss:Index="4" ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s37"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
    <Cell ss:Index="4" ss:StyleID="s27"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s32"/>
    <Cell ss:StyleID="s31"/>
    <Cell ss:StyleID="s33"/>
    <Cell ss:StyleID="s31"/>
    <Cell ss:StyleID="s33"/>
   </Row>
   <Row>
    <Cell ss:StyleID="s34"><Data ss:Type="String">&lt;/jx:forEach&gt;</Data></Cell>
   </Row>
  </Table>
